  <INC file="$PLUGIN_DATA_PATH$/Skins/Controls/Groups/groups.inc"/>
  <INC file="$PLUGIN_DATA_PATH$/Skins/Controls/Meters/meters.inc"/>
  <INC file="$PLUGIN_DATA_PATH$/Skins/Controls/Other/other.inc"/>
  
