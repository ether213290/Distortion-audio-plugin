    <IF condition="$SCRIPT_EDIT_MODE$">
      <INC file="$LM_DIR$/$EDITORS_SUBDIR$/skin_scanner/skin_scanner.inc" />
      <INC file="$LM_DIR$/$EDITORS_SUBDIR$/controls_gen/controls_gen.inc" />
      <INC file="$LM_DIR$/$EDITORS_SUBDIR$/settings/settings.inc" />
      <INC file="$LM_DIR$/$EDITORS_SUBDIR$/global_preferences/global_preferences.inc" />
      <INC file="$LM_DIR$/$EDITORS_SUBDIR$/export/export.inc" />
      <INC file="$LM_DIR$/$EDITORS_SUBDIR$/about_lm_skin/about_lm_skin.inc" />
    </IF>

    <!-- if MAKE_RESIZEABLE then $RE100$ = 100%, otherwise empty-->
    <CELL width="$RE100$" height="$RE100$">
      
      <!-- include toolbar -->
      <INC file="$LM_DIR$/head-toolbar.inc" />
      
      <!-- include main body -->
      <INC file="$LM_DIR$/main-body.inc" />

      <!-- include status bar -->
      <INC file="$LM_DIR$/tail-statusbar.inc" />
    </CELL>

    <!-- corner icon to change window size (visible if MAKE_RESIZEABLE is used) -->
    <CORNER_RESIZER size="16" color="#777777" opacity="1" />
    
  </LAYER_STACK>
